net: "train.prototxt"
#test_iter: 100
# Carry out testing every 500 training iterations.
#test_interval: 500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01
momentum: 0.9
#weight_decay: 0.0005
# The learning rate policy
lr_policy: "fixed"
# Display every 100 iterations
display: 100
# The maximum number of iterations (iterations_in_single_epoch * no_of_mxnet_epochs = 10,000/128*10)
max_iter: 800
# solver mode: CPU or GPU
solver_mode: GPU
